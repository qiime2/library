      {
        "type": "heading",
        "depth": 2,
        "children": [
          {
            "type": "text",
            "value": "Enabling tab completion"
          }
        ]
      },
      {
        "type": "heading",
        "depth": 3,
        "children": [
          {
            "type": "text",
            "value": "Bash"
          }
        ]
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "To enable tab completion in Bash, run the following command or add it to your\n"
          },
          {
            "type": "inlineCode",
            "value": ".bashrc"
          },
          {
            "type": "text",
            "value": "/"
          },
          {
            "type": "inlineCode",
            "value": ".bash_profile"
          },
          {
            "type": "text",
            "value": ":"
          }
        ]
      },
      {
        "type": "code",
        "lang": "bash",
        "value": "source tab-qiime"
      },
      {
        "type": "heading",
        "depth": 3,
        "children": [
          {
            "type": "text",
            "value": "ZSH"
          }
        ]
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "To enable tab completion in ZSH, run the following commands or add them to your\n"
          },
          {
            "type": "inlineCode",
            "value": ".zshrc"
          },
          {
            "type": "text",
            "value": ":"
          }
        ]
      },
      {
        "type": "code",
        "lang": "bash",
        "value": "autoload -Uz compinit && compinit && autoload bashcompinit && bashcompinit && source tab-qiime"
      }
    ]

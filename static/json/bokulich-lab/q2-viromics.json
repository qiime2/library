{
  "readme": {
    "type": "root",
    "children": [
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "A "
          },
          {
            "type": "link",
            "url": "https://qiime2.org",
            "children": [
              {
                "type": "text",
                "value": "QIIME 2"
              }
            ]
          },
          {
            "type": "text",
            "value": " Plugin for detecting viral genomes and assessing their quality."
          }
        ]
      },
      {
        "type": "heading",
        "depth": 2,
        "children": [
          {
            "type": "text",
            "value": "Installation"
          }
        ],
        "identifier": "installation",
        "label": "Installation",
        "html_id": "installation",
        "implicit": true
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "emphasis",
            "children": [
              {
                "type": "text",
                "value": "q2-viromics"
              }
            ]
          },
          {
            "type": "text",
            "value": " is available as part of the QIIME 2 pathogenome distribution. For installation and usage instructions please consult the official "
          },
          {
            "type": "link",
            "url": "https://docs.qiime2.org",
            "children": [
              {
                "type": "text",
                "value": "QIIME 2 documentation"
              }
            ]
          },
          {
            "type": "text",
            "value": "."
          }
        ]
      },
      {
        "type": "heading",
        "depth": 2,
        "children": [
          {
            "type": "text",
            "value": "Usage"
          }
        ],
        "identifier": "usage",
        "label": "Usage",
        "html_id": "usage",
        "implicit": true
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "Start by downloading sample input "
          },
          {
            "type": "link",
            "url": "https://polybox.ethz.ch/index.php/s/9jlQ4oyDWvWyvpB",
            "children": [
              {
                "type": "text",
                "value": "datasets"
              }
            ]
          },
          {
            "type": "text",
            "value": "."
          }
        ]
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "Fetch the checkV database:"
          }
        ]
      },
      {
        "type": "code",
        "lang": "bash",
        "value": "qiime viromics checkv-fetch-db --o-database checkV_db.qza --verbose"
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "Run the CheckV analysis:"
          }
        ]
      },
      {
        "type": "code",
        "lang": "bash",
        "value": "qiime viromics checkv-analysis --i-sequences input_sequences.qza --i-database checkV_db.qza --p-num-threads 4 --output-dir checkV_output --verbose"
      }
    ]
  },

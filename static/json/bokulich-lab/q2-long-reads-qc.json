      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "We have added a new semantic type for the cutadapt log files named “CutadaptLogs”."
          }
        ]
      },
      {
        "type": "code",
        "lang": "",
        "value": "qiime tools import --type 'CutadaptLogs' --input-path dir_with_cutadapt_log_files --output-path cutadapt_logs.qza"
      }
    ]
